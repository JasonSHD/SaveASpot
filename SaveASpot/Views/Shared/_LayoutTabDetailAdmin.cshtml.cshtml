@inherits SaveASpot.Core.Web.Mvc.AdminTabViewPage<dynamic>
@{
	ViewBag.Title = "_LayoutTabDetailAdmin.cshtml";
}

<html>
<head>
	<title>Test</title>
</head>
<body>
	<script>
		@*See "SaveASpot\SaveASpot\Scripts\js\saveaspot.initscript.js"*@
		(function () { var a = []; window.q = function (c, b) { if (b === undefined && c === undefined) { return window.q } if (b == undefined && typeof c === "function") { b = c; c = undefined } if (window.q.init == undefined) { a.push({ filter: c, handler: b }) } return window.q }; window.q.ready = function (b) { window.q(b); return window.q }; window.q.readyCollection = function () { return a } })();
	</script>
	<input type="hidden" id="qFilter" value="adminTabController" />
	<ul>
		@foreach (var tabElement in TabDescriptions)
	{
			<li data-ajaxtab="@Url.Action(tabElement.ActionName, tabElement.ControllerName)" data-ajaxtab-ready="@tabElement.Alias">@tabElement.Title</li>
	}
	</ul>

	<div id="tabContent">
		@RenderBody()
	</div>

	<script src="http://code.jquery.com/jquery-latest.js" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/js/bootstrap.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/js/theme.js")" type="text/javascript"></script>
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCw9fP5c4kc5MiZBvUzLiMc8tY00BU4_i4&sensor=true" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/js/saveaspot.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/js/saveaspot.site.js")" type="text/javascript"></script>

	<script>
		q("adminTabController", function () {
			q.controls.ajaxTab($("#tabContent"));
		});
	</script>
</body>
</html>


