@using Newtonsoft.Json
@using SaveASpot.Core.Security
@model SaveASpot.ViewModels.ViewExtensions.AdminViewModel
<script>
	q(function () {
		q.security.currentUser().fullUser(@Html.Raw(JsonConvert.SerializeObject(Model.User.AsFullUserJson()))).
			anonym(@Html.Raw(JsonConvert.SerializeObject(Model.Anonym.AsUserJson())));
		q.controls.currentAdmin();
	});
</script>
