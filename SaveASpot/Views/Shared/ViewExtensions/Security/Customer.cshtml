@using Newtonsoft.Json
@using SaveASpot.Core.Security
@model SaveASpot.ViewModels.ViewExtensions.CustomerCartViewModel
<script>
	q(function () {
		q.security.currentCustomer().authenticate(@Html.Raw(JsonConvert.SerializeObject(Model.Customer.AsCustomerJson())));
		q.controls.currentCustomer();

		q.cart.currentCart().cart(@Html.Raw(JsonConvert.SerializeObject(Model.Cart.AsCartJson())));
		q.controls.cart();
	});
</script>
