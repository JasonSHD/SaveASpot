@using SaveASpot.Controllers.Artifacts
@using SaveASpot.Core.Web.Mvc.Extensions
@{
	Layout = null;
}

<div class="navbar">
	<div class="navbar-inner">
		<ul class="nav nav-tabs">
			@foreach (var tabElement in Html.For<PhasePageTabAttribute>().OrderBy(e => e.IndexOfOrder))
	 {
				<li>
					@(Html.ButtonForElement<PhasePageTabAttribute>("a", tabElement.Alias, new { href = "#" }))
				</li>
	 }
		</ul>
	</div>
</div>
